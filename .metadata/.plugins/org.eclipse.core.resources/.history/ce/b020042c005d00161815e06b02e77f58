//Thread : 우선 순위 (1~10 까지)
//default :5 (main : 5, Thread1 : 5 , Thread2 :  5)
//Max : 10;
//Min : 1;

class Pth extends Thread{
	@Override
	public void run() {
		for(int i = 0 ; i<10000 ; i++){
			System.out.println("----------");
		}
	}
}
class Pth2 extends Thread{
	@Override
	public void run() {
		for(int i = 0 ; i<10000 ; i++){
			System.out.println("|||||||||||||");
		}
	}
}

public class Ex06_priority {

}
