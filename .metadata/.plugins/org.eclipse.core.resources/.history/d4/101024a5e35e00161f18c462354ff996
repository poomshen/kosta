package teamDTO;


//선수
public class PlayerDTO extends SituationDTO {
	String name; //이름
	int nober; //등번호 
	String position; //포지션
	
	public void allPlayer(String name,int nober ,String position,boolean situation){
		this.name = name;
		this.nober = nober;
		this.position = position;
		this.keyPlayer =situation;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getNober() {
		return nober;
	}
	public void setNober(int nober) {
		this.nober = nober;
	}
	public String getPosition() {
		return position;
	}
	public void setPosition(String position) {
		this.position = position;
	}

}