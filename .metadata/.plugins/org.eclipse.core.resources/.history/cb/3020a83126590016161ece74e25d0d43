//FileInputStream
//FileOutputStream

//대상 파일:

public class Ex03_Stream_File {
	public static void main(String[] args) {
		try {
			FileInputStream fs = new FileInputStream("C:\\Temp\\file.txt");
			int data = 0 ;
			while((data = fs.read())!=-1){
				
			}
		} catch (Exception e) {
			System.out.println(e.getMessage());
		}
	}
}
